{
  "manifest_version": 3,
  "name": "Clarity AI Engine",
  "version": "1.0",
  "description": "Browser-based TensorFlow.js Threat Detection",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
